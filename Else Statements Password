# Access granted with correct password

password='swordfish'
if password=='swordfish':
    print('Access granted')
else:
    print('Wrong password')

# Access denied with incorrect password
password='rosebud'
if password=='swordfish':
    print('Access granted')
else:
    print('Wrong password')

