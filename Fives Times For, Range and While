print('My name is ')
for i in range(5):
    print('Jimmy Five Times '+str(i))
#print Jimmy Fives times 0-4
total=0
for num in range(101):
    total = total+num
print(total)

while i<5:
    print('Jimmy Five Times'+str(i))
    i=i+1

for i in range(12,16):
    print('Jimmy Five Times'+str(i))

for i in range(0,10,2):
    print('Jimmy Five Times'+str(i))
#range function can use 1-3 arguments
